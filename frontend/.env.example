# Frontend Environment Variables (Vite requires VITE_ prefix)

# The local or hosted address of your Express Backend server
VITE_API_BASE_URL=http://localhost:5000

# Firebase Client configuration (found in Firebase Console settings)
VITE_FIREBASE_API_KEY=AIzaSyA-Your-API-Key-Here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=1:your-app-id:web:your-app-hash
VITE_FIREBASE_MEASUREMENT_ID=G-YOUR_MEASUREMENT_ID
